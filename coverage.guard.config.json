{
  "appRootRelativeToGitRepo": "src",
  "excludeKeywords": [
    "// skip-coverage-check",
    "backbone",
    "class SomeUglyClass"
  ],
  "featureNameRegExp": {
    "body": "APP-[0-9]+",
    "flags": "g"
  },
  "excludeFiles": [
    "-test.js",
    ".less",
    "main.ts",
    ".controller.ts",
    ".module.ts"
  ],
  "addedFilesQualityGate": {
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  },
  "changedFilesQualityGate": {
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}